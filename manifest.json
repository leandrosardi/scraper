{
	"name": "LinkedIn Scraper",
	"description": "Simple Extension to Scrape LinkedIn Results, and Push Them to ConnectionSphere for Data Enrichment",
	"version": "1.0",
	"permissions": ["scripting", "declarativeContent", "activeTab", "tabs", "downloads", "*://*/*"],
	"manifest_version": 3,
	"icons": {
	  "48": "/48.png",
	  "128": "/128.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"48": "/48.png",
			"128": "/128.png"
		}
	}
}

  